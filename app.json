{
  "expo": {
    "name": "clearskyrn",
    "slug": "clearskyrn",
    "assetBundlePatterns": [
      "**/*"
    ],
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#ddffe2",
      "image": "./assets/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "androidNavigationBar": {
      "backgroundColor": "#ddffe2",
      "barStyle": "dark-content"
    },
    "androidStatusBar": {
      "backgroundColor": "#ddffe2",
      "barStyle": "dark-content"
    },
    "plugins": [
      "expo-dev-client",
      "expo-splash-screen"
    ]
  }
}